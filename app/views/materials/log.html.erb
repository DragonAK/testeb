<h1> Logs </h1>

<table>
  <thead>
    <tr>
      <th>Material Id</th>
      <th>Operation</th>
      <th>Amount</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.material.name %></td>
        <td><%= log.operacao %></td>
        <td><%= log.qtd %></td>
        <td><%= log.user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'Back', materials_path %>