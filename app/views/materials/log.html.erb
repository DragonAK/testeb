<h1> Logs </h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
<% @logs.each do |log| %>
<tr>
        <td><%= log.user_id %></td>
        <td><%= log.operacao %></td>
        <td><%= log.quant %></td>
</tr>
    <% end %>
  </tbody>
</table>